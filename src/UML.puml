@startuml

!pragma layout smetana

class Member{
e-mail: String
}

class BoardMember

class Event{
date: Date
description: String
assign(l : Location) : void
}

class Conference{
max_attenders: Integer
}

class BoardMeeting

class Location{
description: String
address: String
}

class Person{
name: String
}

class AAUOC{
newLocation(l : Location) : void
newEvent(e : Event) : void
newPerson(p : Person) : void
informEvent(e : Event) : void
register(m : Member,e : Event) : void
}

Event <|-- Conference
Event <|-- BoardMeeting

Person <|-- Member
Member <|-- BoardMember

Location - Event

AAUOC o-- Location
AAUOC o-- Event
AAUOC o-- Person

BoardMeeting - BoardMember

Event - Member

@enduml